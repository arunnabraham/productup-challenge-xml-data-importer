{
    "name": "arunnabraham/xml-data-importer",
    "description": "A Simple Applictaion to import xml data from local or remote to defined formats",
    "type": "library",
    "license": "Unlicenced",
    "autoload": {
        "psr-4": {
            "Arunnabraham\\XmlDataImporter\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Arun Abraham",
            "email": "arunnabraham@gmail.com"
        }
    ],
    "require": {
        "symfony/console": "^6.0",
        "symfony/serializer": "^6.0",
        "eclipxe/xmlschemavalidator": "^3.0",
        "league/csv": "^9.8"
    },
    "config": {
        "allow-plugins": {
            "symfony/runtime": true
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
